    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: lowcode watch",
            "program": "${workspaceFolder}/bin/tcb.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["lowcode", "watch"],
            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug: cloudbase logout",
            "program": "${workspaceFolder}/bin/cloudbase.js",
            "args": ["open"],
            "outFiles": ["${workspaceFolder}/lib/**/*.js"]
        }
    ]
}
